{
  "name": "spencerharston.com",
  "version": "3.0.0",
  "description": "The personal website of Spencer Harston",
  "private": true,
  "type": "module",
  "scripts": {
    "lint": "eslint \"./**/*.js\"",
    "lint:fix": "eslint \"./**/*.js\" --fix",
    "prettier": "prettier . --check",
    "prettier:write": "prettier . --write",
    "start": "run-p start:*",
    "start:eleventy": "eleventy --serve",
    "start:postcss": "postcss src/assets/css/*.css --dir dist/assets/css --watch --verbose",
    "build": "run-s clean build:eleventy build:postcss",
    "build:eleventy": "eleventy",
    "build:postcss": "postcss src/assets/css/*.css --dir dist/assets/css",
    "clean": "rm -rf ./dist",
    "prepare": "husky",
    "books": "scripts/book-manager.js",
    "media": "scripts/media-manager.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sphars/spencerharston.com.git"
  },
  "author": "Spencer Harston",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sphars/spencerharston.com/issues"
  },
  "homepage": "https://github.com/sphars/spencerharston.com#readme",
  "devDependencies": {
    "@11ty/eleventy": "^3.0.0",
    "@11ty/eleventy-img": "^6.0.2",
    "@11ty/eleventy-plugin-rss": "^2.0.3",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.25.0",
    "@fontsource/ibm-plex-mono": "^5.2.5",
    "@fontsource/zalando-sans": "^5.2.1",
    "@inquirer/prompts": "^7.5.0",
    "@sardine/eleventy-plugin-external-links": "^1.4.0",
    "@tabler/icons": "^3.31.0",
    "@tailwindcss/typography": "^0.5.16",
    "autoprefixer": "^10.4.21",
    "canvas": "^3.1.0",
    "commander": "^13.1.0",
    "cssnano": "^7.0.6",
    "eleventy-plugin-og-image": "^4.0.1",
    "eleventy-plugin-reading-time": "^0.0.1",
    "eslint": "^9.25.1",
    "eslint-config-prettier": "^10.1.2",
    "globals": "^16.0.0",
    "html-minifier-terser": "^7.2.0",
    "husky": "^9.1.7",
    "markdown-it-anchor": "^9.2.0",
    "markdown-it-attrs": "^4.3.1",
    "markdown-it-eleventy-img": "^0.10.2",
    "markdown-it-footnote": "^4.0.0",
    "markdown-it-image-figures": "^2.1.1",
    "npm-run-all2": "^7.0.2",
    "postcss": "^8.5.3",
    "postcss-cli": "^11.0.1",
    "prettier": "^3.5.3",
    "prettier-plugin-jinja-template": "^2.1.0",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "tailwindcss": "^3.4.17"
  }
}
